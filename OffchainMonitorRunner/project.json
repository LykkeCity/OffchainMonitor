{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable"
  },
  "dependencies": {
    "OffchainMonitorApi": "1.0.0-*",
    "Core": "1.0.0-*",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8"
    }
  },
  "publishOptions": {
    "include": [
      "docker-compose.yml",
      "Dockerfile",
      ".dockerignore",
      "appsettings.json"
    ]
  }
}
